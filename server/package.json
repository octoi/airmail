{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for spacechat",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/app.ts",
    "dev": "nodemon -r tsconfig-paths/register ./src/app.ts",
    "build": "tsc -p . && resolve-tspaths",
    "lint:check": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write ./src",
    "migrate": "prisma migrate dev --name migration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoi/spacechat.git"
  },
  "bugs": {
    "url": "https://github.com/octoi/spacechat/issues"
  },
  "homepage": "https://github.com/octoi/spacechat#readme",
  "author": "fadhilsaheer",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.15.13",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prisma": "^4.13.0",
    "resolve-tspaths": "^0.8.13",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@prisma/client": "4.13.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^9.0.0"
  }
}
