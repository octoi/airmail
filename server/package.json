    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prisma": "^4.13.0",
    "resolve-tspaths": "^0.8.13",
    "ts-node": "^10.9.1",
